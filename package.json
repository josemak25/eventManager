{
  "name": "eventsmanager",
  "version": "1.0.0",
  "description": "A full stack app allowing users to schedule and manage events at different facilities Users can add, change and delete events Users can perform above actions against facilities. Users can view facilities based on various criteria such as amenities, availability etc Administrators can add facilities",
  "main": "server/app.js",
  "scripts": {
    "start": "nodemon --watch server -r @std/esm server/app.js",
    "start-babel": "nodemon --watch server --exec node_modules/.bin/babel-node server/app.js",
    "build": "babel server -d dist",
    "postbuild": "nodemon --watch dist dist/app.js",
    "test": "mocha --require @babel/register --require @babel/polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ozimos/EventsManager.git"
  },
  "keywords": [
    "API"
  ],
  "author": "Tovieye Moses Ozi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ozimos/EventsManager/issues"
  },
  "homepage": "https://github.com/ozimos/EventsManager#readme",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.36",
    "@babel/core": "^7.0.0-beta.36",
    "@babel/polyfill": "^7.0.0-beta.36",
    "@babel/preset-env": "^7.0.0-beta.36",
    "@babel/register": "^7.0.0-beta.36",
    "@std/esm": "^0.18.0",
    "babel-loader": "^7.1.2",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-joi-validation": "^0.2.1",
    "joi": "^13.0.2",
    "nodemon": "^1.14.3",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0-beta.36",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1"
  },
  "@std/esm": "cjs"
}
